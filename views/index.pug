extends layout

block content
  h1= title
  p Welcome to Amulya Boggavarapu's express page
   button.btn.btn-outline-success(type="button", onclick="goFaster()") Press Me
  div#faster Increase the speed
  script.
    var mph = 20;
    var change = 1;

    function goFaster() {
      mph += change;
      change += 1;
      document.getElementById('faster').innerText = `MPH ${mph} with change ${change}`;
    }
