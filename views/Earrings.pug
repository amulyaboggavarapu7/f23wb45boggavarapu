extends layout

block content
  h1=title
  - let results = [{ material: 'Gold', price: 100, style: 'Stud' },{ material: 'Silver', price: 50, style: 'Hoop' },{ material: 'Diamond', price: 500, style: 'Drop' }]

  .container-fluid
    .row
      each item in results
        .col-sm-2
          p Material: #{item.material} 
          p Price: #{item.price} 
          p Style:#{item.style}
          
  a(href="http://localhost:3000/Earrings") Link to Endpoint
